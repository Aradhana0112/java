class Car{
          String model;
          String color;
          int year;
      
          public Car(String model,String color,int year){
          
          this.model=  model;
          this.color=color;
          this.year= year;
         }
          
         public void display(){
          System.out.println("Car model:"+model);
          System.out.println("Car color:"+color);
          System.out.println("Car yar:"+year);
         }
          
          public static void main(String []args){
           Car myCar= new
           Car("Toyota","Red",2020);
            myCar.display();
         }
}
          

         
