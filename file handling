import java.io. *;
class Createfile
{
 public static void main(String []args)
 
 {
   File f= new File("D://aradhana//abc.txt");
   
   try
  {
     if(f.createNewFile())
     {
        System.out.println("File Created.");
     }
     else
     {
        System.out.println("File is already Exist");
     }
}
catch(Exception e)
{
 System.out.println(e);
}
}
}
